          }
        }
      }
    },
    "data-download": {
      "root": "libs/data-download",
      "sourceRoot": "libs/data-download/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
